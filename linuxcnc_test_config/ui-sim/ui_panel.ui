  <property name="styleSheet">
   <string notr="true">/* Main window background */
QWidget#Form {
    background-color: #1c1c1c;
}

/* Global text color */
QWidget {
    color: white;
    font-family: Arial, sans-serif;
}

/* Tab Widget Styling */
QTabWidget::pane {
    background-color: #121212;
    border: none;
    border-top-left-radius: 40px;
    border-top-right-radius: 40px;
}

QTabBar::tab {
    background: transparent;
    color: #cccccc;
    font-size: 20px;
    font-weight: 500;
    padding: 10px 20px;
    margin-right: 5px;
    border: none;
    min-width: 100px;
}

QTabBar::tab:selected {
    background-color: #b83219;
    color: white;
    border-radius: 5px;
    font-weight: bold;
}

QTabBar::tab:hover:!selected {
    background-color: rgba(184, 50, 25, 0.3);
    color: white;
}

/* Status chips in header */
QLabel#statusChip1, QLabel#statusChip2, QLabel#statusChip3 {
    font-size: 16px;
    padding: 8px 16px;
    border-radius: 9px;
    min-width: 80px;
}

QLabel#statusChip1 {
    background-color: #ffa500;
}

QLabel#statusChip2 {
    background-color: #5da21f;
}

QLabel#statusChip3 {
    background-color: #b83219;
}

QLabel#versionLabel {
    font-size: 16px;
    color: white;
}

/* Button base styles */
QPushButton {
    border: none;
    border-radius: 8px;
    font-size: 24px;
    font-weight: bold;
    color: white;
    min-height: 60px;
    padding: 5px 10px;
}

QPushButton:pressed {
    opacity: 0.8;
}

/* Specific button colors */
QPushButton#startButton, QPushButton#cutButton, QPushButton#liftHeadButton {
    background-color: #5da21f;
}

QPushButton#startButton:hover, QPushButton#cutButton:hover, QPushButton#liftHeadButton:hover {
    background-color: #6db525;
}

QPushButton#pauseButton, QPushButton#lowerHeadButton,
QPushButton#unclampFVButton, QPushButton#unclampMVButton {
    background-color: #2b2b2b;
}

QPushButton#pauseButton:hover, QPushButton#lowerHeadButton:hover,
QPushButton#unclampFVButton:hover, QPushButton#unclampMVButton:hover {
    background-color: #3b3b3b;
}

QPushButton#stopButton, QPushButton#manualStopButton, QPushButton#cancelButton {
    background-color: #b83219;
}

QPushButton#stopButton:hover, QPushButton#manualStopButton:hover, QPushButton#cancelButton:hover {
    background-color: #c8421f;
}

QPushButton#clampFVButton, QPushButton#clampMVButton {
    background-color: #ffa500;
}

QPushButton#clampFVButton:hover, QPushButton#clampMVButton:hover {
    background-color: #ffb520;
}

QPushButton#saveButton {
    background-color: #6e6e6e;
}

QPushButton#saveButton:hover {
    background-color: #7e7e7e;
}

/* G-code preview */
QTextEdit#gcodePreview {
    background-color: #0a0a0a;
    color: #cecece;
    font-family: 'Courier New', monospace;
    font-size: 14px;
    border: 1px solid #2a2a2a;
    border-radius: 5px;
    padding: 10px;
}

/* Visualizer frame */
QFrame#visualizerFrame {
    background-color: #1c1c1c;
    border-radius: 10px;
    border: 1px solid #3c3c3c;
}

/* Manual mode readouts */
QLineEdit {
    background-color: #3a3a3a;
    color: #ffffff;
    font-size: 20px;
    font-weight: 500;
    border-radius: 8px;
    padding: 12px 20px;
    border: 1px solid #505050;
}

QLineEdit:read-only {
    background-color: #3a3a3a;
}

/* Settings frames */
QFrame.settingFrame {
    background-color: #a5a5a5;
    border-radius: 20px;
    padding: 30px;
    min-height: 120px;
}


QCheckBox {
    font-size: 24px;
}

QCheckBox::indicator {
    width: 30px;
    height: 30px;
    border-radius: 15px;
    background-color: #c8c8c8;
}

QCheckBox::indicator:checked {
    background-color: #5da21f;
}

/* Frame backgrounds */
QFrame#topBar {
    background-color: #1c1c1c;
}

QFrame#contentArea {
    background-color: #121212;
    border-top-left-radius: 40px;
    border-top-right-radius: 40px;
}</string>
